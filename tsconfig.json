{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "$/components/ui/*": [
        "./$/components/ui/*"
      ]
    },
    "jsx": "preserve",
    "jsxImportSource": "solid-js"
  }
}